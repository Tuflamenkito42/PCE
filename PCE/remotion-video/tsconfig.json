{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "lib": [
            "dom",
            "esnext"
        ],
        "declaration": true,
        "sourceMap": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "strict": true,
        "isolatedModules": true,
        "noEmit": true,
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "resolveJsonModule": true
    },
    "include": [
        "src"
    ]
}